{
  "name": "atdolar",
  "version": "0.0.1",
  "description": "Aplicación para ver y calcular el precio del dolar hacer conversión de moneda",
  "productName": "AtDolar",
  "author": "steliuz <jesusrosales19@gmail.com>",
  "private": true,
  "scripts": {
    "lint": "eslint --ext .js,.ts,.vue ./",
    "format": "prettier --write \"**/*.{js,ts,vue,scss,html,md,json}\" --ignore-path .gitignore",
    "test": "echo \"No test specified\" && exit 0",
    "dev": "quasar dev",
    "dev:android": "quasar dev -m capacitor -T android",
    "dev:android:ide": "quasar dev -m capacitor -T android --ide",
    "build": "quasar build",
    "favicon": "icongenie generate -i splash.png --skip-trim",
    "icons": "icongenie generate -i splash.png --skip-trim --theme-color 001a45",
    "icons:generate": "icongenie generate -i splash.png --quality 9 -m pwa,capacitor --theme-color 001a45 --splashscreen-color 001a45 --skip-trim",
    "icons:verify": "icongenie verify"
  },
  "dependencies": {
    "@capacitor/android": "^7.2.0",
    "@capacitor/clipboard": "^7.0.0",
    "@capacitor/core": "^7.2.0",
    "@capacitor/keyboard": "^7.0.3",
    "@capacitor/share": "^7.0.0",
    "@quasar/extras": "^1.0.0",
    "apexcharts": "^5.3.5",
    "axios": "^1.2.1",
    "dotenv": "^16.1.4",
    "driver.js": "^1.3.6",
    "leaflet": "^1.9.4",
    "localforage": "^1.10.0",
    "pinia": "^2.0.11",
    "quasar": "^2.6.0",
    "v-money3": "^3.24.1",
    "vue": "^3.0.0",
    "vue-router": "^4.0.0",
    "vue3-apexcharts": "^1.8.0"
  },
  "devDependencies": {
    "@capacitor/cli": "^7.2.0",
    "@quasar/app-vite": "^1.0.0",
    "@types/leaflet": "^1.9.21",
    "@types/node": "^12.20.21",
    "@typescript-eslint/eslint-plugin": "^5.10.0",
    "@typescript-eslint/parser": "^5.10.0",
    "@vue/devtools": "^6.5.0",
    "autoprefixer": "^10.4.2",
    "eslint": "^8.10.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-vue": "^9.0.0",
    "prettier": "^2.5.1",
    "typescript": "^4.5.4"
  },
  "engines": {
    "node": ">=18",
    "npm": ">= 6.13.4",
    "yarn": ">= 1.21.1"
  }
}